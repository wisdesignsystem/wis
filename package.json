{
  "private": true,
  "name": "wis",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "dev": "wis dev",
    "build": "wis build",
    "format": "biome check --write",
    "changelog": "pnpm changeset",
    "version": "pnpm run doc && pnpm changeset version",
    "alpha": "pnpm changeset pre enter alpha && pnpm changeset version && pnpm changeset pre exit",
    "beta": "pnpm changeset pre enter beta && pnpm changeset version && pnpm changeset pre exit",
    "rc": "pnpm changeset pre enter rc && pnpm changeset version && pnpm changeset pre exit",
    "release": "pnpm changeset tag && git push --follow-tags",
    "doc": "typescript-docgen -s ./src/packages -o ./src/definitions.ts && biome check --write",
    "token": "token -n w -s ./src/assets/wis.basic.tokens.json -t blue:./src/assets/wis.blue.tokens.json,dark:./src/assets/wis.dark.tokens.json,orange:./src/assets/wis.orange.tokens.json -o ./src/themes"
  },
  "lint-staged": {
    "**/*.{ts,tsx,json}": "biome check --write"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "1.1.1",
    "@radix-ui/react-collapsible": "1.1.1",
    "@radix-ui/react-context-menu": "2.2.2",
    "@radix-ui/react-dropdown-menu": "2.1.2",
    "@radix-ui/react-toggle": "1.1.0",
    "@radix-ui/react-toggle-group": "1.1.0",
    "@radix-ui/react-tooltip": "1.1.4",
    "@wisdesign/lsicon": "0.0.1",
    "@wiscore/router": "0.0.1",
    "classnames": "2.5.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "i18next": "23.15.1",
    "i18next-browser-languagedetector": "8.0.0",
    "lodash.debounce": "4.0.8",
    "nanoid": "5.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "2.27.5",
    "@changesets/changelog-github": "0.5.0",
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.3",
    "@wisdesign/cli": "0.0.23",
    "@wisdesign/toolkit": "0.0.7",
    "@types/lodash.debounce": "4.0.9",
    "@types/node": "22.10.1",
    "@types/react": "18.3.16",
    "@types/react-dom": "18.3.5",
    "husky": "9.0.11",
    "lint-staged": "15.2.7"
  }
}
